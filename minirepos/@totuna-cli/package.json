{
  "name": "@totuna/cli",
  "version": "0.1.0",
  "description": "â˜¶ TOTUNA | Your PostgreSQL Full Stack Platform",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/antl3x/antl3x",
  "bugs": "https://github.com/antl3x/antl3x/issues",
  "repository": "antl3x/antl3x",
  "license": "AGPL-3.0-only",
  "author": "Antonio Moura (@antl3x)",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "totuna": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && tsc-alias",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "totuna",
    "commands": {
      "strategy": "explicit",
      "target": "./dist/main.js",
      "identifier": "COMMANDS"
    },
    "dirname": "totuna",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@totuna/core": "workspace:^",
    "console-table-printer": "^2.12.0",
    "debug-level": "^3.1.4",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "inquirer": "^9.2.20",
    "mobx": "^6.12.3",
    "ora": "^8.0.1",
    "pg": "^8.11.5",
    "ts-node": "^10",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/debug": "^4.1.12",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/pg": "^8.11.5",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.9.0",
    "shx": "^0.3.4",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.8.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
