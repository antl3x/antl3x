{
  "name": "@totuna/cli",
  "version": "0.1.0",
  "description": "totuna | Your PostgreSQL Full Stack Platform",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/antl3x/antl3x",
  "bugs": "https://github.com/antl3x/antl3x/issues",
  "repository": "antl3x/antl3x",
  "license": "AGPL-3.0-only",
  "author": "Antonio Moura (@antl3x)",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "totuna": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "oclif-bin": "ts-node node_modules/.bin/oclif",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "pnpm oclif-bin manifest && pnpm oclif-bin readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "pnpm run lint",
    "version": "pnpm oclif-bin readme && git add README.md"
  },
  "oclif": {
    "bin": "totuna",
    "commands": "./dist/commands",
    "dirname": "totuna",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "privileges": {
        "description": "Easily manage PostgreSQL privileges (GRANT/REVOKE) using a declarative code-based approach."
      },
      "privileges:pull": {
        "description": "Pull the current state of PostgreSQL privileges from the database and generate descriptive .ts files for easy modification."
      },
      "privileges:push": {
        "description": "Push the changes made in the local .ts files to the database by transforming them into SQL statements and executing them."
      },
      "privileges:diff": {
        "description": "Diff local and remote privilege states to compare."
      },
      "privileges:generate": {
        "description": "Generate .sql migration files based on local schema changes without applying the migrations to the database."
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@totuna/core": "workspace:^",
    "console-table-printer": "^2.12.0",
    "debug-level": "^3.1.4",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "inquirer": "^9.2.20",
    "ora": "^8.0.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/debug": "^4.1.12",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.9.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.8.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
