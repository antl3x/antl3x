{
  "name": "@antl3x/totuna-control",
  "version": "0.0.1",
  "description": "totuna control allows you to control postgresql objects as IaaC",
  "homepage": "https://github.com/antl3x/antl3x/tree/master/minirepos/@totuna-control",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "fix": "pnpm run lint && pnpm run format",
    "build:esm": "esbuild src/index.ts --bundle --platform=node --target=es2015 --outfile=dist/esm/index.js --format=esm",
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --target=es2015 --outfile=dist/cjs/index.js --format=cjs",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "test": "jest"
  },
  "keywords": [
    "postgresql management",
    "row level security",
    "rls",
    "typescript"
  ],
  "author": "Antonio Moura (@antl3x)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/antl3x/antl3x",
    "directory": "/minirepos/func",
    "branch": "master"
  },
  "peerDependencies": {
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.11.5",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "mobx-keystone": "^1.10.1",
    "pg": "^8.11.5"
  }
}
