{
  "name": "@totuna/core",
  "version": "0.0.1",
  "description": "totuna core allows you to control postgresql objects as IaaC",
  "homepage": "https://github.com/antl3x/antl3x/tree/master/minirepos/@totuna-core",
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    "./dist": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./@config": {
      "import": "./dist/esm/@config/index.js",
      "require": "./dist/cjs/@config/index.js",
      "types": "./dist/types/@config/index.d.ts"
    },
    "./@config/*": {
      "import": "./dist/esm/@config/*.js",
      "require": "./dist/cjs/@config/*.js",
      "types": "./dist/types/@config/*.d.ts"
    },
    "./@privileges/*": {
      "import": "./dist/esm/@privileges/*.js",
      "require": "./dist/cjs/@privileges/*.js",
      "types": "./dist/types/@privileges/*.d.ts"
    },
    "./@rootStore": {
      "import": "./dist/esm/@rootStore/index.js",
      "require": "./dist/cjs/@rootStore/index.js",
      "types": "./dist/types/@rootStore/index.d.ts"
    },
    "./@rootStore/*": {
      "import": "./dist/esm/@rootStore/*.js",
      "require": "./dist/cjs/@rootStore/*.js",
      "types": "./dist/types/@rootStore/*.d.ts"
    }
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "fix": "pnpm run lint && pnpm run format",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types && tsc-alias --outDir dist/types",
    "build": "node ./scripts/build.mjs && npm run build:types",
    "test": "jest"
  },
  "keywords": [
    "postgresql management",
    "row level security",
    "rls",
    "typescript"
  ],
  "author": "Antonio Moura (@antl3x)",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/antl3x/antl3x",
    "directory": "/minirepos/func",
    "branch": "master"
  },
  "peerDependencies": {
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.11.5",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "esbuild": "^0.20.2",
    "esbuild-plugin-alias-path": "^2.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@antl3x/func": "workspace:^",
    "@totuna/core": "workspace:^",
    "@types/relaxed-json": "^1.0.4",
    "fast-json-patch": "^3.1.1",
    "mobx": "^6.12.3",
    "mobx-keystone": "^1.10.1",
    "parse-es-import": "^0.6.0",
    "pg": "^8.11.5",
    "relaxed-json": "^1.0.3",
    "tsc-alias": "^1.8.8"
  },
  "pnpm": {
    "patchedDependencies": {
      "parse-es-import@0.6.0": "patches/parse-es-import@0.6.0.patch"
    }
  }
}
