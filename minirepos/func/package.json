{
  "name": "@antl3x/func",
  "version": "1.0.1",
  "description": "func helps define functions with strict type checking and error handling at run-time.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "fix": "pnpm run lint && pnpm run format",
    "build:esm": "esbuild src/index.ts --bundle --platform=node --target=es2015 --outfile=dist/esm/index.js --format=esm",
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --target=es2015 --outfile=dist/cjs/index.js --format=cjs",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "prepare": "npm run build",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  },
  "keywords": [
    "zod",
    "function",
    "type checking",
    "runtime",
    "validation",
    "error",
    "handler",
    "typescript",
    "javascript",
    "nodejs",
    "deno"
  ],
  "author": "Antonio Moura (@antl3x)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/antl3x/antl3x",
    "directory": "minirepos/func",
    "branch": "master"
  },
  "peerDependencies": {
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.3",
    "typescript": "^5.4.5"
  }
}
